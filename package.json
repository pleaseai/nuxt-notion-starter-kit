{
  "name": "nuxt-notion-starter-kit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "pnpm --filter @pleaseai/nuxt-notion-web dev",
    "build": "pnpm --filter @pleaseai/notion-vue build && pnpm --filter @pleaseai/nuxt-notion-web build",
    "build:vue": "pnpm --filter @pleaseai/notion-vue build",
    "build:web": "pnpm --filter @pleaseai/nuxt-notion-web build",
    "preview": "pnpm --filter @pleaseai/nuxt-notion-web preview",
    "lint": "pnpm -r lint",
    "type-check": "pnpm -r type-check",
    "clean": "pnpm -r clean"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}
